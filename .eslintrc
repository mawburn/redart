{
	parserOptions: {
		sourceType: 'module',
		ecmaFeatures: {
			experimentalObjectRestSpread: true,
		},
	},
	"rules": {
		"semi": ["error", "never"],
		'no-unexpected-multiline': 'error',
		"quotes": ["error", "single"],
		indent: ['warn', 2, {SwitchCase: 1}],
		no-trailing-spaces: "error"
	}
}